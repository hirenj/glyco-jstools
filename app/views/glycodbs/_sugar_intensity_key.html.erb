<div id="intensity_key" style="height: 100px;">
<% for res_name in ['GlcNAc','GalNAc','Gal','Glc','Man','NeuAc']%>
<% @sugar = make_sugar(res_name,:ic) %>
<div style="width: 100%; height: 15%; position: relative;">
<div style="height: 100%; width: 10%; position: absolute; left: 0px; top: 0px;"><%= render :partial => "sviewer/render", :locals => { :sugar => @sugar, :padding => 10, :scheme => :composite } %></div><div style="float: left; width: 20%; margin-left: 10%;"><%= res_name %></div>
<% @vals = make_intensity_scale(@sugar.root.prototype.root.attribute('fill').value) %>
<% for val in @vals %><div style="background: <%= val %>; width: 7%; float: left; height: 100%;"> </div><% end %>
</div>
<% end %>
<div style="position: absolute; top: -10%; left: 30%;">100%</div>
<div style="position: absolute; top: -10%; left: 60%;">50%</div>
<div style="position: absolute; top: -10%; left: 90%;">0%</div>
<div style="position: absolute; bottom: 0%; left: 30%;">100%</div>
<div style="position: absolute; bottom: 0%; left: 60%;">50%</div>
<div style="position: absolute; bottom: 0%; left: 90%;">0%</div>
<div style="position: absolute; bottom: -10%; left: 30%;">% of structures residue seen in</div>
</div>